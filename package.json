{
  "name": "boring-moving",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://gaboe.github.io/boring-moving",
  "dependencies": {
    "apollo-client-preset": "^1.0.8",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.8.0",
    "query-string": "^5.1.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-apollo": "^2.1.0-beta.2",
    "react-dom": "^16.2.0",
    "react-google-login": "^3.0.4",
    "react-grid-system": "^3.1.2",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "^2.13.0",
    "react-toastify": "^3.3.4",
    "recharts": "^1.0.0-beta.10",
    "semantic-ui-react": "^0.78.2",
    "styled-components": "^3.1.6",
    "styled-components-ts": "^0.0.8"
  },
  "scripts": {
    "dev": "yarn codegen && yarn start",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "codegen": "yarn get-schema && yarn generate-types",
    "get-schema": "apollo-codegen introspect-schema http://localhost:3000/graphql --output \"./src/generated/schema.json\"",
    "generate-types": "apollo-codegen generate \"./src/gql/**/*.ts\" --schema \"./src/generated/schema.json\" --target typescript --output \"./src/generated/types.ts\" --addTypename"
  },
  "devDependencies": {
    "@types/graphql": "^0.12.4",
    "@types/jest": "22.2.0",
    "@types/node": "^9.4.6",
    "@types/prop-types": "^15.5.2",
    "@types/query-string": "^5.1.0",
    "@types/ramda": "0.25.19",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/react-router-dom": "^4.2.4",
    "@types/react-transition-group": "^2.0.7",
    "@types/recharts": "1.0.17",
    "apollo-codegen": "^0.18.7",
    "gh-pages": "^1.1.0",
    "typescript": "^2.8.0-dev.20180224"
  }
}
